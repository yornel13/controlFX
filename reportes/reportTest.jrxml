     pago.`monto_horas_sobre_tiempo` AS pago_monto_horas_sobre_tiempo,
     pago.`bono` AS pago_bono,
     pago.`transporte` AS pago_transporte,
     pago.`total_bonos` AS pago_total_bonos,
     pago.`vacaciones` AS pago_vacaciones,
     pago.`subtotal` AS pago_subtotal,
     pago.`decimo_tercero` AS pago_decimo_tercero,
     pago.`decimo_cuarto` AS pago_decimo_cuarto,
     pago.`jubilacion_patronal` AS pago_jubilacion_patronal,
     pago.`aporte_patronal` AS pago_aporte_patronal,
     pago.`seguros` AS pago_seguros,
     pago.`uniformes` AS pago_uniformes,
     pago.`total_ingreso` AS pago_total_ingreso,
     pago.`empleado` AS pago_empleado,
     pago.`cedula` AS pago_cedula,
     pago.`empresa` AS pago_empresa,
     pago.`cliente_nombre` AS pago_cliente_nombre,
     pago.`cliente_id` AS pago_cliente_id,
     pago.`usuario_id` AS pago_usuario_id
FROM
     `pago` pago]]>
